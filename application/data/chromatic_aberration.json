equirements" : {"directory" : [{"uri" : "default/color/common/shader/1"}, {"uri" : "default/common/common/shader/1"}]}, "targets" : {"target" : {"api" : "OpenGLES","apiVersion" : "3.0", "shaderprograms" : {"shaderprogram" : {"id" : "chromaticAberrationProgram","type" : "GLSL","version" : "300", "vertexshader" : {"file" : "default/common/common/shader/1/GLSL_ES_300/void_position.vert", "text" : "#version 300 es\n/**\n * Common vertex shader.\n *\n * @author Matthias Trapp\n * @version 1.0\n * @date 2015\n * @copyright Copyright (C) 2015 Digital Masterpieces GmbH, All rights reserved.\n * @namespace Effect::GLSL_ES_300::Common\n * @class Vertex\n *\n */\n/******************************************************************************\n __  _  __ _ _____ __  _    __ __  __   __ _____ ___ ___ ___ _ ___ ______  __\n _\\ / _ _   _/  \\     V  /  \\/' __   _ __ _  _,  __/ _ __/' _/\n v   [/    /\\  _  \\_/  /\\ `._`.    _ v  v_  _ \\_ _`._`.\n__/_\\___ _______ ______/ _ ______ ____\\________/\n\n Copyright (C) 2015 Digital Masterpieces GmbH, All rights reserved\n http://www.digitalmasterpieces.com/\n\n ******************************************************************************/\n\n///////////////////////////////////////////////////////////////////////////////\n// Attributes\n//\n/**\n * Attribute representing vertex position coordinates.\n * @minimum -1.0\n * @maximum  1.0\n*/\nin vec2 a_Position;  ///< Input vertex position coordinates.\n\n///////////////////////////////////////////////////////////////////////////////\n//\n// Varyings\n/**\n * Varying variable representing the texture coordinates for sampling.\n * @minimum 0.0 0.0\n * @maximum 1.0 1.0\n*/\nout vec2 v_TexCoord;    ///< Output vertex texture coordinates.\n\n/**\n * Read vertex coordinate attribute and set output position and texture coordinates\n */\nvoid main()\n{\n\tv_TexCoord = a_Position * 0.5 + 0.5;\n\tgl_Position = vec4(a_Position.xy, 0.0, 1.0 );\n}"}, "fragmentshader" : [{"file" : "default/color/common/shader/1/GLSL_ES_300/chromatic_aberration_main.frag", "text" : "#version 300 es\n/**\n * Chromatic aberration fragment shader.\n *\n * @author Matthias Trapp\n * @version 1.0\n * @date 2020\n * @copyright Copyright (C) 2015-2020 Digital Masterpieces GmbH, All rights reserved.\n * @namespace Effect::GLSL_ES_300::Color\n * @class ChromaticAberrationFragment\n */\n/******************************************************************************\n__  _  __ _ _____ __  _    __ __  __   __ _____ ___ ___ ___ _ ___ ______  __\n _\\ / _ _   _/  \\     V  /  \\/' __   _ __ _  _,  __/ _ __/' _/\n v   [/    /\\  _  \\_/  /\\ `._`.    _ v  v_  _ \\_ _`._`.\n__/_\\___ _______ ______/ _ ______ ____\\________/\n\nCopyright (C) 2015-2016 Digital Masterpieces GmbH, All rights reserved\nhttp://www.digitalmasterpieces.com/\n\n******************************************************************************/\n#ifdef GL_ES\n#  ifdef GL_FRAGMENT_PRECISION_HIGH\n     precision highp float;\n#  else\n     precision mediump float;\n#  endif\n#endif\n\n///////////////////////////////////////////////////////////////////////////////\n// Uniforms\n//\n/**\n * Uniform sampler2D variable for accessing input texture.\n */\nuniform sampler2D u_Texture;        ///< Input texture.\n\n/**\n * Uniform variable for representing the number of iterations (quality) of effect.\n * @minimum 3\n * @maximum 30\n * @default 17\n */\nuniform int   u_NumberOfIterations; ///< Effect quality.\n\n/**\n * Uniform variable for representing the maximum barrel distortion.\n * @minimum 0.0\n * @maximum 5.0\n * @default 0.5\n */\nuniform float u_MaximalDistortion; ///< Maximum barrel distortion.\n\n///////////////////////////////////////////////////////////////////////////////\n// Inputs\n//\n/**\n * Variable representing the texture coordinates for sampling.\n * @minimum 0.0 0.0\n * @maximum 1.0 1.0\n*/\nin vec2 v_TexCoord;   ///< Input texture coordinates.\n\n///////////////////////////////////////////////////////////////////////////////\n// Outputs\n//\n/**\n * Output variable representing the texture coordinates for sampling.\n * @minimum 0.0 0.0 0.0 0.0\n * @maximum 1.0 1.0 1.0 1.0\n*/\nout vec4 o_FragColor;   ///< Output fragment color.\n\n\n/**\n * Computes chromatic aberration effect.\n * @param sampler the texture sampler to fetch from.\n * @param texCoord the 2D texture coordinate in [0.0,1.0]^2.\n * @param numberOfIterations the number of texture fetching iterations in [3, 30].\n * @param maximalDistortion the maximum distortion factor in [0.0, max].\n * @return a chromatic abberation color value.\n */\nvec4 chromaticAberration(\n   const in sampler2D sampler,\n   const in vec2 texCoord,\n   const in int numberOfIterations,\n   const in float maximalDistortion);\n\nvoid main(void)\n{   \n   o_FragColor = \n      chromaticAberration(\n         u_Texture,\n         v_TexCoord,\n         u_NumberOfIterations,\n         u_MaximalDistortion);\n}"}, {"file" : "default/common/common/shader/1/GLSL_COMMON/distortion/distortion_barrel.glsl", "text" : "/**\n * Barrel distortion shader function.\n *\n * @author Matthias Trapp\n * @version 1.0\n * @date 2016\n * @copyright Copyright (C) 2015-2016 Digital Masterpieces GmbH, All rights reserved.\n * @namespace Effect::COMMON::Distortion\n * @class BarrelDistortionFunc\n */\n/******************************************************************************\n__  _  __ _ _____ __  _    __ __  __   __ _____ ___ ___ ___ _ ___ ______  __\n _\\ / _ _   _/  \\     V  /  \\/' __   _ __ _  _,  __/ _ __/' _/\n v   [/    /\\  _  \\_/  /\\ `._`.    _ v  v_  _ \\_ _`._`.\n__/_\\___ _______ ______/ _ ______ ____\\________/\n\nCopyright (C) 2015-2016 Digital Masterpieces GmbH, All rights reserved\nhttp://www.digitalmasterpieces.com/\n\n******************************************************************************/\n\n/**\n * Computes a 2D barrel distortion for a given 2D coordinate.\n * @param coord the 2D coordinate in [0.0,1.0]^2.\n * @param amt the distortion factor in [0.0, max].\n * @return the distorted coordinate.\n */\nvec2 barrelDistortion(\n   const in vec2 coord,\n   const in float amt)\n{\n   vec2 center = coord - 0.5;\n   float dist = dot(center, center);\n   return coord + center * dist * amt;\n}\n"}, {"file" : "default/color/common/shader/1/GLSL_COMMON/chromatic_aberration_func.glsl", "text" : "/**\n * Chromatic aberration shader function.\n *\n * @author Matthias Trapp\n * @version 1.0\n * @date 2016\n * @copyright Copyright (C) 2015-2016 Digital Masterpieces GmbH, All rights reserved.\n * @namespace Effect::GLSL_ES_100::Color\n * @class ChromaticAberrationFunc\n */\n/******************************************************************************\n __  _  __ _ _____ __  _    __ __  __   __ _____ ___ ___ ___ _ ___ ______  __\n _\\ / _ _   _/  \\     V  /  \\/' __   _ __ _  _,  __/ _ __/' _/\n v   [/    /\\  _  \\_/  /\\ `._`.    _ v  v_  _ \\_ _`._`.\n__/_\\___ _______ ______/ _ ______ ____\\________/\n\n Copyright (C) 2015-2016 Digital Masterpieces GmbH, All rights reserved\n http://www.digitalmasterpieces.com/\n\n ******************************************************************************/\n\n/**\n * Computes 2D barrel distortion.\n * @param coord the 2D coordinate in [0.0,1.0]^2.\n * @param amt the distortion factor in [0.0, max].\n * @return the distorted coordinate.\n */\nvec2 barrelDistortion(const in vec2 coord, const in float amt);\n\n\nfloat linearInterpolation(const float t ) \n{\n   return clamp(1.0 - abs( 2.0 * t - 1.0 ), 0.0, 1.0);\n}\n\nfloat remap(\n   const in float t,\n   const in float a,\n   const in float b)\n{\n   return clamp((t - a) / (b - a), 0.0, 1.0);\n}\n\nvec4 spectrumOffset(const in float t) \n{\n   float lo = step(t, 0.5);\n   float hi = 1.0 - lo;\n   float w = linearInterpolation(remap(t, 1.0/6.0, 5.0/6.0));\n\n   vec4 ret = vec4(lo,1.0,hi, 1.) * vec4(1.0-w, w, 1.0-w, 1.0);\n   return pow(ret, vec4(1.0/2.2) );\n}\n \n/**\n * Computes chromatic aberration effect.\n * @param sampler the texture sampler to fetch from.\n * @param texCoord the 2D texture coordinate in [0.0,1.0]^2.\n * @param numberOfIterations the number of texture fetching iterations in [3, 30].\n * @param maximalDistortion the maximum distortion factor in [0.0, max].\n * @return a chromatic abberation color value.\n */ \nvec4 chromaticAberration(\n   const in sampler2D sampler,\n   const in vec2 texCoord,\n   const in int numberOfIterations,\n   const in float maximalDistortion)\n{\n   // Init\n   vec4 sumColor = vec4(0.0);\n   vec4 sumWeights = vec4(0.0);\n   float numberOfIterationsInverse = 1.0 / float(numberOfIterations);\n      \n   for (int i = 0; i < numberOfIterations; ++i)\n   {\n      float t = float(i) * numberOfIterationsInverse;\n      \n      // Compute color weight offset\n      vec4 weight = spectrumOffset(t);\n      \n      // Sum up      \n      sumWeights += weight;\n      sumColor += weight * texture(sampler, barrelDistortion(texCoord, maximalDistortion * t));\n   }\n      \n   return sumColor / sumWeights;\n}"}]}}, "textures" : {"texture2d" : {"id" : "OUTPUTTEXTURE"}}, "passes" : {"pass" : {"id" : "chromaticAberrationPass", "passattributes" : {"passattribute" : {"name" : "shaderprogram", "value" : "chromaticAberrationProgram"}}, "passinputs" : {"passinput" : [{"id" : "u_Texture","type" : "sampler", "value" : "INPUTTEXTURE"}, {"id" : "u_NumberOfIterations","type" : "int", "value" : "17"}, {"id" : "u_MaximalDistortion","type" : "float", "value" : "0.5"}]}, "passoutputs" : {"passoutput" : {"id" : "chromaticAberrationPassOutput0", "value" : "OUTPUTTEXTURE"}}}}, "parameters" : {"valueparameter" : [{"id" : "distortion", "setpassinput" : {"pass" : "chromaticAberrationPass","passinput" : "u_MaximalDistortion"}}, {"id" : "quality", "setpassinput" : {"pass" : "chromaticAberrationPass","passinput" : "u_NumberOfIterations"}}]}}}}}}}}}}}